{
  "name": "github-asana",
  "version": "4.0.0",
  "description": "Action to integrate git with asana",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "transform": "rm -rf ./build && mkdir -p build && bun build --target node ./src/action.ts  > build/action.tmp",
    "build": "yarn transform && cat ./patch/bun-require.patch > build/action.js && cat build/action.tmp >> build/action.js && rm build/action.tmp",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insurify/github-actions.git"
  },
  "keywords": [
    "github-actions",
    "asana",
    "git"
  ],
  "author": "Ali Sajid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/insurify/github-actions/issues"
  },
  "homepage": "https://github.com/insurify/github-actions#readme",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^1.1.0",
    "asana": "^2.0.3"
  },
  "devDependencies": {
    "@octokit-next/endpoint": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "jest": "^26.0.1",
    "typescript": "^5.1.6"
  }
}
