{
  "name": "github-asana",
  "version": "4.0.0",
  "description": "Action to integrate git with asana",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "patch": "bun run ./patch/patch.ts",
    "build": "rm -rf ./dist && bun build --target=node --outfile ./dist/index.js ./src/index.ts && yarn patch",
    "format": "prettier --check --write .",
    "format:fail": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cctv1005s/github-asana-action.git"
  },
  "keywords": [
    "github-actions",
    "asana",
    "git"
  ],
  "author": "Li Yang(cctv1005s@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cctv1005s/github-asana-action/issues"
  },
  "homepage": "https://github.com/cctv1005s/github-asana-action",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^1.1.0",
    "asana": "^2.0.3"
  },
  "devDependencies": {
    "@octokit-next/endpoint": "^2.7.1",
    "jest": "^29.6.3",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  }
}
